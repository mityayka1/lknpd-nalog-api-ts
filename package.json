{
  "name": "lknpd-nalog-api-ts",
  "version": "1.0.0",
  "description": "TypeScript клиент для API lknpd.nalog.ru (Мой налог) - формирование и отмена чеков для самозанятых",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "lknpd-authorize": "dist/cli/authorize.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "authorize": "npx tsx src/cli/authorize.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "nalog",
    "moy-nalog",
    "lknpd",
    "самозанятый",
    "npd",
    "налог",
    "чек",
    "receipt",
    "fns",
    "фнс",
    "api"
  ],
  "author": "@Mityayka1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mityayka1/lknpd-nalog-api-ts.git"
  },
  "homepage": "https://github.com/mityayka1/lknpd-nalog-api-ts#readme",
  "bugs": {
    "url": "https://github.com/mityayka1/lknpd-nalog-api-ts/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
